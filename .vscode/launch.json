    "version": "0.2.0",
    "configurations": [
            {
                "type": "node",
                "request": "launch",
                "name": "Command Line Backtest",
                "program": "${workspaceFolder}\\gekko.js",
                "args": [
                    "--config",
                    "config.js",
                    "--backtest"
                ]
            },
            {
                "type": "node",
                "request": "launch",
                "name": "Launch Program",
                "program": "${workspaceFolder}\\gekko.js",
                "args": ["--ui"]
            },
            {
                "type": "node",
                "request": "attach",
                "name": "Attach by Process ID",
                "processId": "${command:PickProcess}"
            }
